<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<ScrollPane xmlns="http://javafx.com/javafx/11.0.1">
    <AnchorPane>
        <TabPane>
            <Tab text="SERVICENAME">
                <AnchorPane>
                    <HBox alignment="CENTER">
                        <VBox>
                            <HBox alignment="CENTER">
                                <Label alignment="TOP_LEFT" contentDisplay="CENTER" text="Conversations">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Button alignment="TOP_LEFT" mnemonicParsing="false" text="General Settings">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                            </HBox>
                            <TextField promptText="Search for Conversation">
                                <font>
                                    <Font size="18.0"/>
                                </font>
                            </TextField>
                            <TabPane side="BOTTOM" tabClosingPolicy="UNAVAILABLE">
                                <Tab text="Contacts">
                                    <AnchorPane>
                                        <ScrollPane>
                                            <AnchorPane/>
                                        </ScrollPane>
                                    </AnchorPane>
                                </Tab>
                                <Tab text="Conversations">
                                    <AnchorPane>
                                        <ScrollPane>
                                            <AnchorPane/>
                                        </ScrollPane>
                                    </AnchorPane>
                                </Tab>
                                <Tab text="Archived">
                                    <AnchorPane>
                                        <ScrollPane>
                                            <AnchorPane/>
                                        </ScrollPane>
                                    </AnchorPane>
                                </Tab>
                            </TabPane>
                        </VBox>
                        <VBox>
                            <HBox alignment="CENTER_RIGHT">
                                <Label text="CONVERSATIONNAME">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Button mnemonicParsing="false" text="Call">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                                <Button mnemonicParsing="false" text="Conversation Settings">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                            </HBox>
                            <ScrollPane>
                                <AnchorPane/>
                            </ScrollPane>
                            <HBox alignment="CENTER_LEFT">
                                <MenuButton mnemonicParsing="false" text="Special Message">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="Voice Message"/>
                                        <MenuItem mnemonicParsing="false" text="File"/>
                                    </items>
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </MenuButton>
                                <TextField promptText="Write a message">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </TextField>
                                <Button mnemonicParsing="false" text="Send">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                            </HBox>
                        </VBox>
                    </HBox>
                </AnchorPane>
            </Tab>
        </TabPane>
    </AnchorPane>
</ScrollPane>
